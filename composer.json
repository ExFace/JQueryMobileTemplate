{
	"name" : "ExFace/JQueryMobileTemplate",
	"description" : "UI Template for the ExFace web application plattform based on jQuery Mobile",
	"type" : "library",
	"license" : "GPL-3.0+",
	"authors" : [{
			"name" : "Andrej Kabachnik",
			"email" : "kabachnik@gmail.com",
			"role" : "Creator"
		}
	],
	"support" : {
		"issues" : "https://github.com/exface/JQueryMobileTemplate/issues"
	},
	"require" : {
		"exface/core" : ">=1.0",
		"oomphinc/composer-installers-extender": "*",
		"bower-asset/jquery": "1.8.x",
		"bower-asset/jquery-mobile" : "~1.4.5",
		"bower-asset/masonry" : "~4",
		"bower-asset/datatables.net" : "~1.10.12",
		"npm-asset/screenfull" : "~3.0"
	},
	"extra": {
        "installer-types": ["library"],
        "installer-paths": {
            "bower-asset/{$name}-1.8/": ["bower-asset/jquery"]
        }
    }
}